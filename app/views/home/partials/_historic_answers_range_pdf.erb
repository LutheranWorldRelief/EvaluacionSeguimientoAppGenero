<% section.questions.order('id ASC').each_with_index  do |question, number| %>
  <div class="col-12">
    <% answer = user_answers.by_section_id_question_id(section.id, question.id) %>
    <%= render partial: "home/partials/historic_answers_range_piechart_pdf", locals: {
          title: question.question.html_safe,
          data: user_answers.piechart_men_women_data_by_answer(answer),
          colors: user_answers.piechart_colors(),
          description: user_answers.piechart_description(answer),
          recommendation: question.recommendation.html_safe,
       } %>
  </div>
<% end %>